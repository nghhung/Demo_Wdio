  "scripts": {
    "lint": "eslint --ext .tsx,.ts,.js,.jsx src/",
    "lint:fix": "yarn lint --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "TZ=utc LANG=en_US react-scripts test --env=jsdom-sixteen",
    "eject": "react-scripts eject",
    "test-ci": "yarn test --coverage",
    "ci": "touch ./src/aws-exports.js && yarn lint && yarn test-ci",
    "generate-graphql-types": "graphql-codegen --config codegen.yml",
    "wdio": "wdio e2e/config/wdio.conf.js",
    "report:copy": "node e2e/config/convert.historyAllureReport.js",
    "report:allure": "npx allure",
    "report:generate": "yarn report:copy && yarn report:allure generate --clean ./e2e/allure-report/allure-result/ -o ./e2e/allure-report/allure-report",
    "report:open": "yarn report:allure open e2e/allure-report/allure-report"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "CI=true yarn test-ci --ci --bail"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .tsx,.ts,.js,.jsx"
    ]
  },
